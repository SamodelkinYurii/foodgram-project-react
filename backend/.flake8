[flake8]
exclude = 
    tests/,
    */migrations/,
    venv/,
    */venv/,
    env/
    */env/,
    .git, 
    __pycache__,
    __init__.py,
max-line-length = 79
ignore =     
    W503,
    F811,
    I001,
    I002,
    I003,
    I004,
    I005,
per-file-ignores =
    */settings.py:E501
    */manage.py:E501